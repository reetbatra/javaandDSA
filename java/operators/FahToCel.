package lecture1;
import java.util.Scanner;
public class FahToCel {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int f = s.nextInt();
		//int cel = (5/9)*(f-32);
		//System.out.println(cel);
		// check for 100, answer is zero (wrong),
		// this (/) operator we are using is made to
		//divide int by int and give answer as int
		int cel = (5 * (f - 32)/9);
		System.out.println(cel);
		
		
        System.out.print(9.0/5);
        System.out.println('A' + 1); //66
        System.out.println('A' + 'B'); //195
	}

}
