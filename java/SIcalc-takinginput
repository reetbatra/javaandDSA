package lecture1;
import java.util.Scanner;
public class SIcalculator {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
       long p= s.nextLong();
       int r= s.nextInt();
       int t=s.nextInt() ;
       int si =(p*r*t)/100;
       System.out.println(si);
       
       
	}

}
