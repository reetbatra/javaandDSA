
import java.util.Scanner;
public class Solution {

	public static void main(String[] args) {
		Scanner S=new Scanner(System.in);
		String name = S.next();
        
        char c = name.charAt(0);
        int m1 = S.nextInt();
        int m2 = S.nextInt();
        int m3 = S.nextInt();
        //
        System.out.println(c);
        int avg = (m1 + m2 + m3)/3;
        System.out.println(avg);
        
        }

}
