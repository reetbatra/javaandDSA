public class add2num {

	public static void main(String[] args) {
        
		int i=10, j=15;
        int k= i+j;
        System.out.println(k);
       i=13;
       k= i+j;
        System.out.println(k); 
       short s = 123;
        long l=1233456;
        float f;
        double d=1.23;
        char c='a';
       boolean b=true;
        boolean otherB=false;
        
	}

}
